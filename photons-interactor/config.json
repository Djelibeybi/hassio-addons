{
    "name": "Photons Interactor",
    "version": "0.12.4",
    "slug": "photons-interactor",
    "description": "Photons Interactor provides a local RESTful API for LIFX devices",
    "url": "https://photons.delfick.com/apps/interactor/homeassistant.html",
    "init": false,
    "hassio_api": true,
    "arch": [
      "amd64",
      "aarch64",
      "armhf",
      "armv7",
      "i386"
    ],
    "host_network": true,
    "apparmor": false,
    "map": [
      "config:rw"
    ],
    "ports": {
        "6100/tcp": 6100
      },
      "ports_description": {
        "6100/tcp": "Photons Interactor REST API"
    },
    "options": {
        "enable_logging": false,
        "override_config_file": false
    },
    "schema": {
        "enable_logging": "bool",
        "override_config_file": "bool"
    }
  }
