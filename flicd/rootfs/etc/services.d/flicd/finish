#!/usr/bin/execlineb -S0
# shellcheck shell=bash
# ==============================================================================
# Home Assistant Community Add-on: flicd
# Take down the S6 supervision tree when flicd ends
# ==============================================================================
if -n { s6-test $# -ne 0 }
if -n { s6-test ${1} -eq 256 }

s6-svscanctl -t /var/run/s6/services
